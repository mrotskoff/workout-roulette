{
  "expo": {
    "name": "Workout Roulette",
    "slug": "workout-roulette",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "updates": {
      "enabled": false
    },
    "plugins": ["expo-sqlite", "expo-asset", "expo-audio"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ghostrunnertech.workoutroulette.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ghostrunnertech.workoutroulette.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    }
  }
}
